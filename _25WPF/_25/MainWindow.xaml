<Window x:Class="_25.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_25"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="print" Content="Check" Width="100" Height="30" FontSize="16" Margin="400,200,0,0" Click="passwordValidator"></Button>
        <TextBox x:Name="input" Margin="0,0,200,200" Height="40" Width="200" Text="Enter password" FontSize="16"></TextBox>
        <TextBlock x:Name="output" Margin="0,0,200,0" Width="200" Height="40" FontSize="16" Background="Wheat"></TextBlock>
    </Grid>
</Window>
